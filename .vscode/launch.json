  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastCLAM: FastAPI for CLAMS",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "module": "poetry",
      "args": [
        "run",
        "uvicorn",
        "fastclam.app:app",
        "--reload",
        "--host",
        "0.0.0.0",
        "--log-level",
        "debug"
      ],
      "jinja": false,
      "justMyCode": false
    }
  ]
}
